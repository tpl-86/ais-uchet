{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: AIS-UCHET",
            "type": "python",
            "request": "launch",
            "module": "src.main",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "QT_DEBUG_PLUGINS": "1"
            }
        },
        {
            "name": "Python: Debug Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": ["-v", "tests"],
            "console": "integratedTerminal"
        }
    ]
}

